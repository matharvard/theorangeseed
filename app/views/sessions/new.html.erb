<% form_for(:session, :url => session_path) do |form| %>
  <div class="field text_field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  
  <div class="field text_field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  
  <div class="actions">
    <%= form.submit "Sign in" %>
  </div>
<% end %>
